{
	"name": "ember-jsonapi-docs-generator",
	"version": "3.0.0",
	"description": "Generates API docs for the official ember projects",
	"homepage": "https://github.com/ember-learn/ember-jsonapi-docs",
	"bugs": "https://github.com/ember-learn/ember-jsonapi-docs/issues",
	"repository": "ember-learn/ember-jsonapi-docs",
	"license": "MIT",
	"author": "Sivakumar Kailasam @sivakumar-kailasam",
	"contributors": [
		"Sivakumar Kailasam <siva@rubberduck.engineering>",
		"Todd Jordan",
		"Ember.JS Learning team"
	],
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"bin": {
		"ember-docgen": "./bin/run"
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
		"backup": "./bin/run backup",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"postpack": "rm -f oclif.manifest.json",
		"start": "./bin/run full-run",
		"test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"posttest": "tslint -p test -t stylish",
		"version": "oclif-dev readme && git add README.md"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"dependencies": {
		"@ember-learn/released-js-docs": "0.0.0",
		"@oclif/command": "^1",
		"@oclif/config": "^1",
		"@oclif/plugin-help": "^2",
		"bluebird": "^3.7.1",
		"cli-ux": "^5.3.3",
		"command-exists": "^1.2.8",
		"compare-versions": "^3.5.1",
		"deepmerge": "^4.2.2",
		"download": "^7.1.0",
		"execa": "^3.3.0",
		"file-extension": "^4.0.5",
		"fs-extra": "^8.1.0",
		"gatsby-remark-vscode": "^1.3.1",
		"glob": "^7.1.6",
		"hard-rejection": "^2.1.0",
		"hast-util-raw": "^5.0.1",
		"http-server": "^0.11.1",
		"inflected": "^2.0.4",
		"lodash": "^4.17.15",
		"mdast-util-to-hast": "^7.0.0",
		"rehype-stringify": "^6.0.1",
		"remark-parse": "^7.0.2",
		"request": "^2.88.0",
		"rev-file": "^3.0.0",
		"rmfr": "^2.0.0",
		"targz": "^1.0.1",
		"tslib": "^1",
		"unified": "^8.4.2",
		"yuidoc-to-jsonapi": "^0.4.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1",
		"@oclif/test": "^1",
		"@oclif/tslint": "^3",
		"@types/bluebird": "^3.5.29",
		"@types/chai": "^4",
		"@types/command-exists": "^1.2.0",
		"@types/deepmerge": "^2.2.0",
		"@types/fs-extra": "^8.0.1",
		"@types/inflected": "^1.1.29",
		"@types/lodash": "^4.14.148",
		"@types/mocha": "^5",
		"@types/node": "^12",
		"@types/pretty-time": "^1.1.0",
		"@types/rmfr": "^2.0.0",
		"chai": "^4.2.0",
		"eslint": "6.6.0",
		"eslint-plugin-node": "10.0.0",
		"eslint-plugin-prettier": "3.1.1",
		"globby": "^10",
		"husky": "^3.0.9",
		"mocha": "^6.2.2",
		"nyc": "^14",
		"prettier": "1.19.1",
		"pretty-quick": "^2.0.1",
		"pretty-time": "^1.1.0",
		"ts-node": "^8",
		"tslint": "^5",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.7"
	},
	"cacheDirectories": [
		"tmp"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "ember-docgen",
		"plugins": [
			"@oclif/plugin-help"
		]
	}
}
