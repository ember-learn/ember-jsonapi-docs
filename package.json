{
    "name": "ember-jsonapi-docs-generator",
    "version": "0.2.0",
    "description": "Generates json api docs from ember's yuidocs",
    "main": "index.js",
    "engines": {
        "node": ">= 7"
    },
    "cacheDirectories": [
        "tmp"
    ],
    "scripts": {
        "autoformat": "standard --fix index.js lib/*.js",
        "start": "node --use-strict index.js",
        "test": "standard && mocha test",
        "greet": "echo 'hi'"
    },
    "author": "Stanley Stuart <stanley+npm@stan.li>",
    "license": "MIT",
    "devDependencies": {
        "chai": "^4.0.2",
        "mocha": "^3.4.2",
        "standard": "^10.0.2",
        "standard-format": "^2.2.4"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.1",
        "fs-extra": "^3.0.1",
        "glob": "^7.1.2",
        "graceful-fs": "^4.1.11",
        "highlight.js": "^9.12.0",
        "human-size": "^1.1.0",
        "inflected": "^2.0.1",
        "lodash": "^4.17.4",
        "marked": "^0.3.6",
        "minimist": "^1.2.0",
        "mkdir-promise": "^1.0.0",
        "mkdirp": "^0.5.1",
        "node-zopfli": "^2.0.2",
        "ora": "^1.2.0",
        "rev-file": "^2.0.0",
        "rimraf": "^2.6.1",
        "rsvp": "^3.5.0",
        "s3": "^4.4.0",
        "semver-extra": "^2.0.1",
        "semver-utils": "^1.1.1",
        "yuidoc-to-jsonapi": "^0.4.1"
    },
    "standard": {
        "globals": [
            "describe",
            "beforeEach",
            "afterEach",
            "before",
            "after",
            "it"
        ]
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ember-learn/ember-jsonapi-docs.git"
    },
    "bugs": {
        "url": "https://github.com/ember-learn/ember-jsonapi-docs/issues"
    },
    "homepage": "https://github.com/ember-learn/ember-jsonapi-docs#readme"
}
