{
	"name": "ember-jsonapi-docs-generator",
	"version": "2.0.0",
	"description": "Generates json api docs from ember's yuidocs",
	"homepage": "https://github.com/ember-learn/ember-jsonapi-docs#readme",
	"bugs": {
		"url": "https://github.com/ember-learn/ember-jsonapi-docs/issues"
	},
	"repository": "git+https://github.com/ember-learn/ember-jsonapi-docs.git",
	"license": "MIT",
	"author": "Stanley Stuart <stanley+npm@stan.li>",
	"contributors": [
		"Todd Jordan",
		"Sivakumar Kailasam <siva@rubberduck.engineering>",
		"Ember.JS Learning team"
	],
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"backup": "node -r esm backup.js",
		"gen": "node -r esm generate-local.js",
		"greet": "echo 'hi'",
		"upload-tmp-to-aws": "node -r esm lib/upload-tmp-to-aws.js",
		"server": "http-server -p 5050 --cors tmp",
		"serve": "yarn server",
		"start": "node index.js",
		"test:mocha": "mocha -r esm test",
		"lint": "eslint lib/**/*.js index.js test/**.js",
		"test": "yarn lint && yarn test:mocha"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.2",
		"command-exists": "^1.2.8",
		"compare-versions": "^3.5.1",
		"download": "^7.1.0",
		"ember-rfc176-data": "^0.3.12",
		"esm": "^3.2.25",
		"execa": "^2.0.4",
		"fs-extra": "^8.1.0",
		"glob": "^7.1.4",
		"hard-rejection": "^2.1.0",
		"highlight.js": "^10.4.1",
		"http-server": "^0.11.1",
		"inflected": "^2.0.4",
		"keyfinder": "^1.0.0",
		"lodash": "^4.17.21",
		"marked": "^0.7.0",
		"minimist": "^1.2.6",
		"mkdir-promise": "^1.0.0",
		"mkdirp": "^0.5.1",
		"ora": "^3.4.0",
		"pkg-versions": "^2.1.0",
		"rev-file": "^3.0.0",
		"rimraf": "^2.6.3",
		"rsvp": "^4.8.5",
		"semver-compare": "^1.0.0",
		"semver-extra": "^3.0.0",
		"semver-utils": "^1.1.4",
		"yuidoc-to-jsonapi": "^0.4.1"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chalk": "^2.4.2",
		"eslint": "6.4.0",
		"eslint-plugin-node": "10.0.0",
		"eslint-plugin-prettier": "3.1.1",
		"husky": "^3.0.5",
		"mocha": "^6.2.0",
		"prettier": "1.18.2",
		"pretty-quick": "^1.11.1"
	},
	"cacheDirectories": [
		"tmp"
	]
}
